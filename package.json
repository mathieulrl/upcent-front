{
  "name": "upcent-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ledgerhq/live-app-sdk": "^0.5.0",
    "@ledgerhq/wallet-api-client": "^1.9.1",
    "@ledgerhq/wallet-api-client-react": "^1.4.12",
    "@ledgerhq/wallet-api-simulator": "^1.2.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@ton/crypto": "^3.3.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^19.1.7",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "typescript": "^5.9.2",
    "viem": "^2.33.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"yarn start:onramp-server\" \"react-scripts start\"",
    "start:main": "react-scripts start",
    "start:onramp-server": "cd onramp/server-app && npm start",
    "start:onramp-client": "cd onramp && npm start",
    "start:all": "yarn start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:all": "yarn install && cd onramp/server-app && npm install && cd .. && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
